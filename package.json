{
  "name": "mall-api",
  "version": "1.0.0",
  "description": "商城后端API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:wait-services": "node scripts/wait-for-services.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "init:testdata": "node src/scripts/initTestData.js",
    "init:rbac": "node src/scripts/initRbacData.js",
    "deploy": "./scripts/deploy.sh",
    "deploy:gitee": "./scripts/deploy-gitee.sh",
    "deploy:dev": "./scripts/deploy.sh dev",
    "deploy:tools": "./scripts/deploy.sh tools",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:status": "docker-compose ps",
    "docker:backup": "./scripts/deploy.sh backup",
    "docker:clean": "./scripts/deploy.sh clean"
  },
  "dependencies": {
    "@koa/multer": "^4.0.0",
    "@koa/router": "^12.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-helmet": "^8.0.1",
    "koa-static": "^5.0.0",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "nodemailer": "^7.0.5",
    "rate-limiter-flexible": "^2.4.2",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "sharp": "^0.34.3",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "mall",
    "api",
    "koa",
    "mysql",
    "redis"
  ],
  "author": "",
  "license": "MIT"
}
